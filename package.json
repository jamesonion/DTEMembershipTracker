    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "hoist-non-react-statics": "^3.3.2",
    "mini-create-react-context": "^0.4.1",
    "mocha": "^8.3.0",
    "mongoose": "^5.11.19",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.3",
    "resolve-pathname": "^3.0.0",
    "scheduler": "^0.20.1",
